{
  "CollectionName": "c_markers",
  "Fields": [
    {
      "Key": "_id",
      "Type": "String",
    },
    {
      "Key": "_openid", // 埋点人识别码
      "Type": "Number"
    },
    {
      "Key": "_gamecode", // 游戏识别码 (与_geoname确认一个点)
      "Type": "String"
    },
    {
      "Key": "_geoname", // 埋点名字 (与_gamecode确认一个点)
      "Type": "GeoPoint"
    },
    {
      "Key": "_geo", // 埋点对象，含特殊字段 extend:{text,img}
      "Type": "Object"
    }
  ],
  "Index": [
    {
      "Name": "_id_",
      "Unique": true,
      "OnField": [
        {
          "Key": "_id",
          "Order": "ASC"
        }
      ]
    },
    {
      "Name": "_gamecode_",
      "Unique": false,
      "OnField": [
        {
          "Key": "_gamecode",
          "Order": "DESC"
        }
      ]
    },
    {
      "Name": "_geo_",
      "Unique": true,
      "OnField": [
        {
          "Key": "_id",
          "Order": "ASC"
        },
        {
          "Key": "_geoname",
          "Order": "ASC"
        },
        {
          "Key": "_gamecode",
          "Order": "DESC"
        }
      ]
    }
  ],
  "Auth": "674"
}